<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/friendsFragment">

    <fragment
        android:id="@+id/articleFragment"
        android:name="com.zoe.firestoregroup5.ArticleFragment"
        android:label="fragment_article"
        tools:layout="@layout/fragment_first">
        <action
            android:id="@+id/action_FirstFragment_to_friendsFragment"
            app:destination="@id/friendsFragment" />
    </fragment>
    <fragment
        android:id="@+id/friendsFragment"
        android:name="com.zoe.firestoregroup5.FriendsFragment"
        android:label="fragment_friends"
        tools:layout="@layout/fragment_friends" >
        <action
            android:id="@+id/action_friendsFragment_to_FirstFragment"
            app:destination="@id/articleFragment" />
    </fragment>
</navigation>